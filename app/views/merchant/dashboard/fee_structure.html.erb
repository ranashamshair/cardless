<!-- dashboard header Start -->
<!-- <div class="dash_body"> -->
    <div id="dash_header">
        <div class="container-fluid">
          <div class="row align-items-center">
            <div class="dash_header_left ml-5">
              <div class="welcom_heading">
                Welcome, </div>
              <div class="small_head pl-2">Your Accounts Dashboard</div>
            </div>
            <div class="dash_header_right ml-auto">
              <div class="toggle"></div>
              <ul>
                <li><a href="#">
                    <div class="notify_icon shadow-sm"><i class="fas fa-bell"></i></div>
                    
                  </a></li>
                <li><a href="#">
                    <div class="user_icon shadow-sm"><i class="far fa-user-circle"></i></div>
                  </a></li>
              </ul>
            </div>
          </div>
        </div>
      </div>
      <!-- dashboard header End -->
<div class="fee_structure_section">
            <div class="container">
                <nav aria-label="breadcrumb bg-info">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="#">Accounts</a></li>
                        <li class="breadcrumb-item active" aria-current="page">Fee Structure</li>
                    </ol>
                </nav>


                <!-- Table Start -->
                <div class="fee_structure_box">
                    <div class="heading mb-4">Fee Structure</div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="box_main shadow">
                                <div class="box-data">
                                    <div class="row text-center">
                                        <div class="col-lg-4 pt-2 pb-2">
                                            <div class="trans_type_data">Bank - Credit Card</div>
                                        </div>
                                        <div class="col-lg-4 pt-2 pb-2">
                                            <div class="trans_type_data"><%= number_to_currency(number_with_precision(@fee.sale_credit_bank, precision: 2, delimiter: ',')) %></div>
                                        </div>
                                        <div class="col-lg-4 pt-2 pb-2">
                                            <div class="trans_type_data"><%= number_with_precision(@fee.sale_credit_bank_percent, precision: 2, delimiter: ',') %>%</div>
                                        </div>
                                    </div>
                                </div>

                                <div class="box-data child mt-3 mb-3">
                                    <div class="row text-center">
                                        <div class="col-lg-4 pt-2 pb-2">
                                            <div class="trans_type_data">Bank - Debit Card</div>
                                        </div>
                                        <div class="col-lg-4 pt-2 pb-2">
                                            <div class="trans_type_data"><%= number_to_currency(number_with_precision(@fee.sale_debit_merchant, precision: 2, delimiter: ',')) %></div>
                                        </div>
                                        <div class="col-lg-4 pt-2 pb-2">
                                            <div class="trans_type_data"><%= number_with_precision(@fee.sale_debit_bank_percent, precision: 2, delimiter: ',') %>%</div>
                                        </div>
                                    </div>
                                </div>

                                <div class="box-data">
                                    <div class="row text-center">
                                        <div class="col-lg-4 pt-2 pb-2">
                                            <div class="trans_type_data">Merchant - Credit Card</div>
                                        </div>
                                        <div class="col-lg-4 pt-2 pb-2">
                                            <div class="trans_type_data"><%= number_to_currency(number_with_precision(@fee.sale_credit_merchant, precision: 2, delimiter: ',')) %></div>
                                        </div>
                                        <div class="col-lg-4 pt-2 pb-2">
                                            <div class="trans_type_data"><%= number_with_precision(@fee.sale_credit_merchant_percent, precision: 2, delimiter: ',') %>%</div>
                                        </div>
                                    </div>
                                </div>

                                <div class="box-data child mt-3 mb-3">
                                    <div class="row text-center">
                                        <div class="col-lg-4 pt-2 pb-2">
                                            <div class="trans_type_data">Merchant - Debit Card</div>
                                        </div>
                                        <div class="col-lg-4 pt-2 pb-2">
                                            <div class="trans_type_data"><%= number_to_currency(number_with_precision(@fee.sale_debit_merchant, precision: 2, delimiter: ',')) %></div>
                                        </div>
                                        <div class="col-lg-4 pt-2 pb-2">
                                            <div class="trans_type_data"><%= number_with_precision(@fee.sale_debit_merchant_percent, precision: 2, delimiter: ',') %>%</div>
                                        </div>
                                    </div>
                                </div>

                                <div class="box-data mt-3 mb-3">
                                    <div class="row text-center">
                                        <div class="col-lg-4 pt-2 pb-2">
                                            <div class="trans_type_data">Withdraw Fee</div>
                                        </div>
                                        <div class="col-lg-4 pt-2 pb-2">
                                            <div class="trans_type_data"><%= number_to_currency(number_with_precision(@fee.withdraw, precision: 2, delimiter: ',')) %></div>
                                        </div>
                                        <div class="col-lg-4 pt-2 pb-2">
                                            <div class="trans_type_data">0.00%</div>
                                        </div>
                                    </div>
                                </div>

                                <div class="box-data child mt-3 mb-3">
                                    <div class="row text-center">
                                        <div class="col-lg-4 pt-2 pb-2">
                                            <div class="trans_type_data">Reserve Fee</div>
                                        </div>
                                        <div class="col-lg-4 pt-2 pb-2">
                                            <div class="trans_type_data">$0.00</div>
                                        </div>
                                        <div class="col-lg-4 pt-2 pb-2">
                                            <div class="trans_type_data"><%= number_with_precision(@fee.reserve, precision: 2, delimiter: ',') %>%</div>
                                        </div>
                                    </div>
                                </div>

                                

                            </div>
                        </div>
                    </div>
                </div>
                <!-- Table End -->

            </div>
        </div>